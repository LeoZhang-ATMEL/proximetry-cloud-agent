{% import 'templates/param/code.jinja2' as code with context %}

{# This really has no use if we rely on user_label #}
//   <s.100> Name
//   <i> A name which defines the parameter in the code
//   <id> prox_param_name
#define {{ code.get_name_define() }}  "param"

//   <o> Type
//                        <1=> bool
//                        <2=> int8
//                        <3=> int16
//                        <4=> int32
//                        <5=> uint8
//                        <6=> uint16
//                        <7=> uint32
//                        <8=> float
//                        <9=> string
//   <i> config parameter type
//   <id> prox_param_type
#define {{ code.get_type_define() }}  1

//   <o> ID <1-65535>
//   <i> An id is crucial and must be consistent with an id on the Proximetry Portal
//   <id> prox_param_id
#define {{ code.get_id_define() }}  1

